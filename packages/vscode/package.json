{
	"name": "development-toolkit",
	"displayName": "Foretag Development Toolkit",
	"description": "Foretag themes, icons, extensions, languages, snippets and more for VS Code",
	"version": "0.1.4",
	"keywords": [
		"rust",
		"themes",
		"extension",
		"foretag"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./build/extensions/extensions.js",
	"publisher": "foretag",
	"icon": "assets/images/foretag_logo_yellow.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/ForetagInc/vscode-toolkit.git"
	},
	"engines": {
		"vscode": "^1.66.1"
	},
	"scripts": {
		"prebuild": "rm -rf build && npm run compile",
		"prepublish": "npm run prebuild && npm run icons:build",
		"icons:build": "npm run icons:copy && node build/icons/index.js",
		"icons:copy": "mkdir -p build/svgs && cp -R icons/svgs build",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"categories": [
		"Programming Languages",
		"Themes",
		"Extension Packs",
		"Visualization",
		"Other"
	],
	"extensionPack": [
		"rust-lang.rust-analyzer",
		"masterco.master-css-language-service",
		"redhat.vscode-xml",
		"redhat.vscode-yaml",
		"dtsvet.vscode-wasm",
		"editorconfig.editorconfig",
		"please-build.plz-vscode",
		"hashicorp.terraform",
		"ms-kubernetes-tools.vscode-kubernetes-tools",
		"ms-azuretools.vscode-docker",
		"tauri-apps.tauri-vscode",
		"lottiefiles.vscode-lottie",
		"joshbolduc.story-explorer",
		"joshbolduc.commitlint",
		"CucumberOpen.cucumber-official",
		"vadimcn.vscode-lldb",
		"serayuzgur.crates",
		"tamasfe.even-better-toml"
	],
	"contributes": {
		"commands": [
			{
				"command": "foretag-rust.setRustTarget",
				"title": "Rust: Set Rust Target"
			},
			{
				"command": "foretag-security.toggleEnvLine",
				"title": "Toggle ENV line"
			},
			{
				"command": "foretag-security.pasteEnvLine",
				"title": "Paste Line"
			},
			{
				"command": "foretag-security.insertEnvLine",
				"title": "Insert Line"
			}
		],
