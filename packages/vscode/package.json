		"languages": [
			{
				"id": "dotenv",
				"extensions": [
					".env",
					".env-sample",
					".env.example",
					".env.template",
					".env.local",
					".env.dev",
					".env.dev.local",
					".env.build",
					".env.build.local",
					".env.development",
					".env.development.local",
					".env.test",
					".env.test.local",
					".env.testing",
					".env.qa",
					".env.qa.local",
					".env.uat",
					".env.uat.local",
					".env.stag",
					".env.stag.local",
					".env.stage",
					".env.stage.local",
					".env.staging",
					".env.staging.local",
					".env.live",
					".env.live.local",
					".env.production",
					".env.production.local",
					".env.prod",
					".env.prod.local"
				],
				"filenamePatterns": [
					"?*env"
				],
				"filenames": [
					"env"
				],
				"configuration": "./languages/env/language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "dotenv",
				"scopeName": "source.env",
				"path": "./languages/env/syntaxes/env.tmLanguage"
			}
		],
		"iconThemes": [
			{
				"id": "foretag-cosmos-icons",
				"label": "Foretag: Cosmos",
				"path": "./build/icons.json"
			}
		],
		"themes": [
			{
				"label": "Foretag: Cosmos",
				"uiTheme": "vs-dark",
				"path": "./themes/cosmos.json"
			},
			{
				"label": "Foretag: Indigo",
				"uiTheme": "vs-dark",
				"path": "./themes/indigo.json"
			}
		]
